{
  "expo": {
    "name": "Espacio Tecno",
    "slug": "EspacioTecno",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "androidStatusBar": {
    "barStyle": "dark-content",
    "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "ar.gob.bahia.espaciotecno",
        "versionCode": 8,
        "permissions": ["CAMERA"]
    }
  }
}
