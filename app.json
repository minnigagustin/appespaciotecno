{
  "expo": {
    "name": "Espacio Tecno",
    "slug": "EspacioTecno",
    "version": "1.0.0",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Para escanear QR necesitamos permiso en tu celular."
        }
      ]
    ],
    "icon": "./assets/icononoresponsive.png",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconoresponsive.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "ar.gob.bahia.espaciotecno",
        "versionCode": 4,
        "permissions": ["CAMERA"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
